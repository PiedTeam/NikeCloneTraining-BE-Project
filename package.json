{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "nike-clone-training-fe",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV=development npx nodemon",
        "build": "rimraf ./dist && tsc && tsc-alias",
        "start:dev": "cross-env NODE_ENV=development node dist/src/index.js",
        "start:prod": "cross-env NODE_ENV=production node dist/src/index.js",
        "prettier:check": "prettier ./src/**/*.{tsx,ts,js,jsx} --check",
        "prettier:fix": "prettier ./src/**/*.{tsx,ts,js,jsx} --write",
        "eslint:check": "eslint ./src --ext .ts,.tsx,.js,.jsx --max-warnings 0",
        "eslint:fix": "eslint ./src --ext .ts,.tsx,.js,.jsx --max-warnings 0 --fix",
        "eslint:check-allow-warning": "eslint ./src --ext .ts,.tsx,.js,.jsx",
        "check-conflict-prettier-eslint": "npx eslint-config-prettier src/App.tsx --print-config",
        "check-types": "tsc --noemit"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/crypto-js": "^4.2.2",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/lodash": "^4.17.0",
        "@types/node": "^20.12.3",
        "@types/nodemailer": "^6.4.14",
        "@types/nodemailer-express-handlebars": "^4.0.5",
        "@types/otp-generator": "^4.0.2",
        "@types/passport-facebook": "^3.0.3",
        "@types/passport-google-oauth20": "^2.0.14",
        "@typescript-eslint/parser": "^7.5.0",
        "@typescript-eslint/eslint-plugin": "^7.5.0",
        "@types/validator": "^13.11.10",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.0.11",
        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.8",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.4.3",
        "@eslint/eslintrc": "^3.0.2",
        "@tsconfig/node16": "^16.1.3"
    },
    "dependencies": {
        "cross-env": "^7.0.3",
        "cookie-parser": "^1.4.6",
        "cookie-session": "^2.1.0",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "cryptojs": "^2.5.3",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-rate-limit": "^7.2.0",
        "express-validator": "^7.0.1",
        "http-status-codes": "^2.3.0",
        "jsonwebtoken": "^9.0.2",
        "libphonenumber-js": "^1.11.2",
        "lodash": "^4.17.21",
        "mongodb": "^6.5.0",
        "nodemailer": "^6.9.13",
        "nodemailer-express-handlebars": "^6.1.2",
        "otp-generator": "^4.0.1",
        "passport": "^0.7.0",
        "passport-facebook": "^3.0.0",
        "passport-google-oauth20": "^2.0.0",
        "twilio": "^5.0.4",
        "validator": "^13.12.0"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
